{
  "name": "muntele",
  "version": "0.1.0",
  "description": "Baby's second raymarching implementation",
  "scripts": {
    "dev": "parcel src/index.html --no-cache",
    "clean": "rm -rf .parcel-cache && rm -rf dist",
    "build": "parcel build src/index.html --no-cache --no-source-maps --dist-dir dist",
    "serve": "npx http-serve dist",
    "fft": "node fft.js && parcel src/fftTool/index.html --no-cache",
    "capture": "node ./capture.js",
    "convert": "ffmpeg -framerate 60 -pattern_type glob -i './capture/*.png' -i './src/tekkno2.ogg' -c:v libx264 -pix_fmt yuv420p -crf 21 capture.mp4",
    "test:glb": "node -r @swc-node/register test/testGlb.ts"
  },
  "author": "Ilkka HÃ¤nninen",
  "license": "ISC",
  "devDependencies": {
    "@parcel/transformer-glsl": "^2.12.0",
    "@swc-node/register": "^1.6.8",
    "@types/node": "^17.0.25",
    "parcel": "^2.12.0",
    "puppeteer": "^22.3.0",
    "typescript": "^4.6.3"
  }
}
