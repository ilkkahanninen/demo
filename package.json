  "devDependencies": {
    "@parcel/transformer-glsl": "^2.4.1",
    "@types/node": "^17.0.25",
    "parcel": "^2.4.1",
    "puppeteer": "^19.7.2",
    "typescript": "^4.6.3"
  }
}
